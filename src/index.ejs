<html>
<head>
    <meta charset="utf-8" />
    <title><%= htmlWebpackPlugin.options.title %></title>
</head>
<body>
    <div id="modalWrapper">
        <div class="modal">
            <h3 class="modal-header"></h3>
            <div id="modalContent"></div>
            <div class="button-wrapper">
                <button type="button" id="modalCloseButton">OK</button>
            </div>
        </div>
    </div>
    <div class="app-container">
        <div class="game-wrapper">
            <h4 id="timer"></h4>
            <h5 id="points">0</h5>
            <div class="cards-wrapper" id="cardsWrapper"></div>
            <div class="start-button-wrapper">
                <button type="button" id="startButton">Start game!</button>
            </div>
        </div>
    </div>
</body>
</html>